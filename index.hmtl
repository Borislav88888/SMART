                container.classList.add('hidden');
                taskbarApp.classList.remove('active');
            }
            
            // –û–±–Ω–æ–≤–ª—è–µ–º —Ä–∞–∑–º–µ—Ä—ã –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–æ–≤
            updateContainersSize();
