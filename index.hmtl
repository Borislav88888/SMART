        }

        function toggleApp(appId) {
            const container = document.querySelector(`.speech-container[data-app="${appId}"]`);
            const taskbarApp = document.querySelector(`.taskbar-app[data-app="${appId}"]`);
            
            if (container.classList.contains('hidden')) {
